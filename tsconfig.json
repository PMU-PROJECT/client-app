{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    // "noImplicitAny": true
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "buildOptions": {
    "ignoreBuildErrors": true
  }
}
